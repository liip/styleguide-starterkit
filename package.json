{
  "name": "styleguide-starterkit",
  "version": "0.1.0",
  "description": "A starterkit to create styleguides with Fractal and Webpack.",
  "keywords": [
    "styleguide",
    "toolkit",
    "boilerplate",
    "starterkit",
    "fractal"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/liip/styleguide-starterkit.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "parallelshell 'node ./server.js' 'webpack-dev-server --config webpack.dev.js'",
    "build": "npm run build:fractal && npm run build:webpack",
    "build:webpack": "NODE_ENV=production webpack --config webpack.prod.js",
    "build:fractal": "NODE_ENV=production fractal build"
  },
  "devDependencies": {
    "@frctl/fractal": "^1.1.7",
    "@frctl/nunjucks": "^1.0.3",
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "node-sass": "^4.7.2",
    "parallelshell": "^3.0.2",
    "postcss": "^6.0.16",
    "postcss-cli": "^4.1.1",
    "postcss-loader": "^2.0.10",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "svg-sprite-loader": "^3.5.4",
    "svgo": "^1.0.3",
    "svgo-loader": "^2.1.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11"
  ]
}
