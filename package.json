{
  "name": "styleguide-starterkit",
  "version": "0.3.0",
  "description": "A starterkit to create styleguides with Fractal and Webpack.",
  "keywords": [
    "styleguide",
    "toolkit",
    "boilerplate",
    "starterkit",
    "fractal"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/liip/styleguide-starterkit.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development parallelshell 'node ./server.js' 'webpack-dev-server --hot'",
    "build": "npm run build:fractal && npm run build:webpack",
    "build:webpack": "NODE_ENV=production webpack",
    "build:fractal": "NODE_ENV=production fractal build"
  },
  "devDependencies": {
    "@frctl/fractal": "^1.1.7",
    "@frctl/nunjucks": "^1.0.3",
    "autoprefixer": "^8.3.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "file-loader": "^1.1.11",
    "marked": "^0.3.19",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.3",
    "parallelshell": "^3.0.2",
    "postcss": "^6.0.21",
    "postcss-cli": "^5.0.0",
    "postcss-loader": "^2.1.4",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.20.3",
    "svg-sprite-loader": "^3.7.3",
    "svgo": "^1.0.5",
    "svgo-loader": "^2.1.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11"
  ]
}
