    "concurrently": "^4.1.0",
    "conventional-changelog-cli": "^2.0.12",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "file-loader": "^3.0.1",
    "marked": "^0.6.1",
    "mini-css-extract-plugin": "^0.5.0",
