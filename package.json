    "build:webpack": "NODE_ENV=production webpack",
    "build:fractal": "NODE_ENV=production fractal build",
    "release": "release-it",
    "deploy": "echo \"No deploy script specified, skippingâ€¦\" && exit 0"
  },
  "devDependencies": {
